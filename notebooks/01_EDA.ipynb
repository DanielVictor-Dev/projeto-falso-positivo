        "# EDA - Análise Exploratória de Dados\n",
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "df = pd.read_csv('../data/transacoes.csv')\n",
        "\n",
        "sns.countplot(data=df, x='canal', hue='fraude')\n",
        "plt.title(\"Fraudes por Canal\")\n",
        "plt.show()\n"
      ]
    }
  ]
